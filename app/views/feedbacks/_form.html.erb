<%= form_for @feedback,html: {multipart: true} do |f| %>
    <div class="form-group">
        <%= f.text_field :title, class:"input", placeholder:t("posts.new.post_name")%>  
    </div>
    
    <div class="form-group">
         <h5><%= f.label :body%></h5>
        <%= f.text_field :body %>  
    </div>

<% end %>